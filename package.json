{
  "name": "octopoda",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "webpack --color --config webpack.config.prod.js",
    "build:dev": "webpack --color --config webpack.config.dev.js",
    "start": "wait-on dist/server.js && node dist/server",
    "start:debug": "wait-on dist/server.js && node --inspect-brk dist/server",
    "buildstart": "npm-run-all --silent --print-label clean build start",
    "buildstart:dev": "npm-run-all --silent --print-label clean --parallel build:dev start",
    "buildstart:debug": "npm-run-all --silent --print-label clean --parallel build:dev start:debug",
    "lint": "npm-run-all --silent --print-label lint:tsc lint:eslint",
    "lint:fix": "npm-run-all --silent --print-label lint:tsc lint:eslintfix",
    "lint:tsc": "tsc --noEmit -p tsconfig.build.json",
    "lint:eslint": "eslint --color --max-warnings 0 ./ --ext ts",
    "lint:eslintfix": "eslint --fix --color --max-warnings 0 ./ --ext ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.13",
    "@nestjs/core": "^6.10.13",
    "@nestjs/platform-express": "^6.10.13",
    "@nestjs/swagger": "^4.1.2",
    "aedes": "^0.39.0",
    "influx": "^5.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.10.13",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "8.5.4",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "5.20.1",
    "typescript": "3.7.4",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
